{
    "name": "osw3/odyssey-discovery",
    "description": "A simple Micro Framework",
    "license": "MIT",
    "version": "1.1",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Arnaud BODEL",
            "email": "contact@osw3.net"
        }
    ],
    "require": {
        "altorouter/altorouter": "^1.2",
        "league/plates": "^3.3",
        "ircmaxell/random-lib": "^1.2",
        "ircmaxell/password-compat": "^1.0",
        "league/url": "^3.3",
        "phpmailer/phpmailer": "^6.0",
        "cocur/slugify": "^3.1",
        "ramsey/uuid": "^3.7.3",
        "somework/composer-symlinks": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Odyssey\\": "odyssey/",
            "": "src/"
        },
