{
    "name": "osw3/odyssey-discovery",
    "type": "project",
    "description": "A simple Micro Framework",
    "keywords": ["framework"],
    "homepage": "https://github.com/osw3/odyssey-discovery",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud BODEL",
            "email": "contact@osw3.net"
        }
    ],
    "support": {
        "doc": "http://odyssey.osw3.net/"
    },
    "require": {
        "osw3/odyssey-core-php": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "somework/composer-symlinks": {
            "symlinks": {
                "app/resources/public/assets/fonts/DeadIsland": "web/assets/fonts/DeadIsland"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/osw3/odyssey-discovery"
        }
    ]
}
